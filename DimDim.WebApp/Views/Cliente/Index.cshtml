@model IList<Cliente>

@{
    ViewData["title"] = "Listagem de clientes";
}

<h2>Clientes cadastrados</h2>
<table class="table table-striped">
    <tr>
        <th>Nome</th>
        <th>CPF</th>
        <th>E-mail</th>
        <th>Telefone</th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.Nome</td>
        <td>@item.Cpf</td>
        <td>@item.Email</td>
        <td>@item.Telefone</td>
        <td>
    </td>

    </tr>}
</table>
<a asp-action="cadastrar" class="btn btn-outline-info">Cadastrar</a>